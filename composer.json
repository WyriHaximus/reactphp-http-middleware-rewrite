{
    "name": "wyrihaximus/react-http-middleware-rewrite",
    "description": "A internal path rewrite middleware for react/http",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "psr/http-message": "^1",
        "react/http": "^1"
    },
    "require-dev": {
        "brunty/cigar": "^1.12",
        "psr/log": "^1.1",
        "wyrihaximus/async-test-utilities": "^4.0"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Http\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Http\\Middleware\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
